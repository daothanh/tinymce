    "require": {
        "filament/filament": "^3.0-stable",
        "unisharp/laravel-filemanager": "^2.6"
    },
    "license": "Private",
    "autoload": {
        "psr-4": {
            "Daothanh\\Tinymce\\": "src/"
        }
    },
    "extra": {
